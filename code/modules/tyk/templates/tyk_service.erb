[Unit]
Description=Tyk
Wants=basic.target
After=basic.target network.target

[Service]
Type=forking
ExecStart=<%= @tyk_parent_dir %>/tyk.start
Restart=on-failure
LimitNOFILE=<%= @limit_nofile %>
LimitNPROC=<%= @limit_nproc %>
User=<%= @user %>
Group=<%= @group %>
PIDFile=<%= @tyk_parent_dir %>/tyk-gateway.pid

[Install]
WantedBy=multi-user.target
